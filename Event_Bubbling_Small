<html>
<head>
<title>Event Bubbling</title>
</head>
<body>
<div id="parent">
<button>Parent</button> <br> <br>
<button id="child1">Child1</button>
</div>

<script>
document.getElementById("parent").addEventListener('click', function(){ alert("Parent button is clicked")
})
document.getElementById("child1").addEventListener('click', function(){ alert("Child1 button is clicked")
})

</script>
</body>
</html>
